package com.lc.sofa.core.framework.basis.context;


import com.lc.sofa.core.framework.basis.exception.SofaException;
import com.lc.sofa.core.framework.constants.ExceptionConstants;;
/**
 * 
 * 平台上下文环境异常类
 * @author  YZ
 * @version 1.0, 2013-10-21
 * @since 1.0, 2013-10-21
 */
public class ContextException extends SofaException{

	/**
	 * 
	 */
	private static final long serialVersionUID = -515235587294029947L;

	public String getExceptionCode() {

		
		return ExceptionConstants.SOFA_CONTEXT_EXCEPTION_CODE;
	}
	
	public ContextException(String message){
		   super(message);
		
	}
	
  public ContextException(String message, Throwable cause){
 	    super(message,cause);
		
	}
  
  public ContextException(Throwable cause){
 	     super(cause);
		
	}

}
